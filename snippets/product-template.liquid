    margin: 13px 0 0;
    padding: 0;
    list-style: none;
}
.payments_icons ul li {
    padding: 0 15px 0 0;
}
.payments_icons ul li:last-child {
    padding-right: 0;
}

.payments_icons ul {
	justify-content: center;

}
.payments_icons ul li img {
	width: 95px;
}
.text-bold-payment {
